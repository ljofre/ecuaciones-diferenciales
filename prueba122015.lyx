\begin_layout Problem
Resolver 
\begin_inset Formula $\frac{dy}{dx}=\left(-5x+y\right)^{2}-4$
\end_inset

 haciendo la sustitución de variable auxiliar 
\begin_inset Formula $u=-5x+y$
\end_inset


\end_layout

\begin_layout Problem
Sea 
\begin_inset Formula 
\begin{eqnarray*}
u & = & -5x+y\\
\frac{du}{dx} & = & -5+\frac{dy}{dx}\\
\implies\\
\frac{dy}{dx} & = & \frac{du}{dx}+5
\end{eqnarray*}

\end_inset


\end_layout

\begin_layout Problem
10 puntos
\end_layout

\begin_layout Problem
por lo que la ecuación queda de la forma 
\begin_inset Formula 
\begin{eqnarray*}
\frac{du}{dx}+5 & = & u^{2}-4\\
\frac{du}{dx} & = & u^{2}-9\\
\frac{du}{u^{2}-9} & = & dx\\
\frac{du}{\left(u-3\right)\left(u+3\right)} & = & dx\\
\mbox{10 puntos}\\
\mbox{fracciones parciales}\\
\left(\frac{\frac{1}{6}}{u-3}+\frac{-\frac{1}{6}}{u+3}\right)du & = & dx\\
\left(\frac{1}{6}\int\frac{1}{u-3}-\frac{1}{6}\int\frac{1}{u+3}\right)du & = & \int dx\\
\frac{1}{6}\ln\left|u-3\right|-\frac{1}{6}\ln\left|u+3\right| & = & x+c\\
\ln\left|u-3\right|-\ln\left|u+3\right| & = & 6\left(x+c\right)\\
\ln\frac{\left|u-3\right|}{\left|u+3\right|} & = & 6\left(x+c\right)\\
\ln\frac{\left|-5x+y-3\right|}{\left|-5x+y+3\right|} & = & 6\left(x+c\right)
\end{eqnarray*}

\end_inset


\end_layout

